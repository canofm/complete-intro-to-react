{
  "name": "complete-intro-to-react",
  "version": "1.0.0",
  "description": "A Complete Intro to React workshop",
  "main": "server.js",
  "repository": "git@github.com:canofm/complete-intro-to-react.git",
  "author": "Federico Cano <canofedericomartin@gmail.com>",
  "license": "MIT",
  "scripts": {
    "api": "node ratingsAPI.js",
    "build": "webpack",
    "dev": "webpack-dev-server",
    "format":
      "prettier --write --single-quote --print-width=100 --parser=flow --tab-width=2 \"js/**/*.{js,jsx}\"",
    "lint": "eslint **/*.{js,jsx} --quiet",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:update": "jest -u",
    "server": "NODE_ENV='server' node server.js",
    "watch": "webpack --watch"
  },
  "jest": {
    "testEnvironment": "jest-environment-node",
    "snapshotSerializers": ["jest-serializer-enzyme"],
    "collectCoverageFrom": ["**/*.jsx"]
  },
  "dependencies": {
    "axios": "^0.16.2",
    "babel-plugin-dynamic-import-webpack": "1.0.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-register": "6.24.1",
    "compression": "1.6.2",
    "cors": "2.8.3",
    "express": "4.15.3",
    "lodash": "4.17.4",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-addons-perf": "15.4.2",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-thunk": "^2.2.0",
    "styled-components": "^2.1.2",
    "webpack": "2.6.1",
    "webpack-dev-middleware": "1.10.2",
    "webpack-hot-middleware": "^2.19.1"
  },
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-dynamic-import-node": "^1.0.2",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.24.1",
    "babel-preset-env": "^1.5.1",
    "babel-preset-react": "^6.24.1",
    "enzyme": "^2.9.1",
    "eslint": "^4.7.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-config-prettier": "^2.1.1",
    "eslint-config-react": "^1.1.7",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-flow": "2.29.1",
    "eslint-plugin-flowtype": "2.33.0",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-prettier": "^2.1.1",
    "eslint-plugin-react": "^7.0.1",
    "flow-bin": "0.47.0",
    "jest": "^21.1.0",
    "jest-serializer-enzyme": "^1.0.0",
    "moxios": "^0.4.0",
    "prettier": "^1.7.0",
    "react-hot-loader": "3.0.0-beta.6",
    "react-test-renderer": "^15.6.1",
    "webpack-dev-server": "2.4.5"
  }
}
